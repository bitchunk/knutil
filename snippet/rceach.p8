--rceach [rceach() performs a two-dimensional loop in a single function.]
--v0.1
--@shiftalow / bitchunk
function rceach(r,f)
 local x,y,w,h=unpack(
  type(r)=='table'
	  and r
	  or split(r,' ',true)
 )
 for y=y,y+h-1 do
  for x=x,x+w-1 do
   f(x,y,r)
  end
 end
end

function _update()
end

function _draw()
	cls()
--[[
 --arguments--
  [table] {
  									x-offset, y-offset,
           width,    height
          },
	 [function] processing
]]--
	rceach({16,16,32,32},function(x,y,r)
		local c=(x+y)%8+8
		pset(x,y,c)
	end)

--[[
 --arguments--
 [string (space separate)] "
 								 x-offset y-offset
          width height
								 ",
 [function] processing
]]--
	rceach('0 0 32 32',function(x,y,r)
		local c=((x/4)%8+(y/4)%8+cos(time()/10)*50)%16
		sset(x+8,y,c)
		
		local m=x%4+1+(y%4*16)
		mset(x,y,m)
	end)
	map(0,0,56,56,8,8)
end

