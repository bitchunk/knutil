version 41
__lua__
--tonorm
--v0.1
--@shiftalow / bitchunk

--[[
	- tonorm         -- normalize argument values (strings) to the correct type.
	- @param  any v  -- value to normalize type.
	- @return any    -- normalized value of type.
	- @description
		-- hexadecimal numbers are converted to decimal numbers.
]]--

function tonorm(v)
if v=='false' then return false
elseif v=='nil' then return nil
end
return v=='true' or tonum(v) or v
end

----
cls()
local t,f,n,d,h,s
					='true'
					,'false'
					,'nil'
					,'32'
					,'0x10'
					,'string value'

?'::tonorm::\n\n \f7[initial values]'
for i,v in pairs({t,f,n,d,h,s}) do
?v..'  \t\#3\fbtype:['..type(v)..']',6
end

?'\t⬇️\n\t⬇️\n\t⬇️\n [normalize values]',7
for i,v in pairs({t,f,n,d,h,s}) do
v=tonorm(v)
?tostr(v)..'  \t\#3\fbtype:['..type(v)..']',6
end

