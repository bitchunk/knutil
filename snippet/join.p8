--join [join strings with a delimiter.]
--v0.3a
--@shiftalow / bitchunk

--[[
	- join strings with a delimiter
	- @param  string d delimiter
	- @param  string s,...
	- @return string joined string value
	- @description
		- a nil argument returns an empty string.
 
]]--

function join(d,s,...)
	for v in all(s and {...}) do
		s..=d..v
	end
	return s or ''
end

----
cls()
?join('====','----','----','----','----\n')

?join(' ',unpack({'test','[join]','is','ok!\n\n'}))

?join('\fa◆\f6','c','o','m','p','l','e','t','e\n')

?join('\-e=\-e',unpack(split('----------------------------','')))

s=join('\+fk',unpack(split('llllllll','')))

?join('\-e'..join('\+ec',unpack(split('」」」」」」','')))..'\-e「\-f',s,s,s)

