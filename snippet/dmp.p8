--dmp [dmp() dumping table values]
--v0.3
--@shiftalow / bitchunk
_dmpl={}
function dmp(v,x)
	local tstr={
		number="#"
		,string="$"
		,boolean="%"
	}
	tstr["function"]
		,tstr["nil"]
		,tstr.table
	="*","!",'{'

local p,s=true,''
if x==nil then x=0 color(6) cls()
else
s="\*"..sub(tostr(x,1),6,6)..' '
end
v=type(v)=='table' and v or {v}
for i,str in pairs(v) do
	if type(str)=='table' then
	 add(_dmpl,s..i..tstr[type(str)])
		dmp(str,x+1)
	 add(_dmpl,s..'}')
 p=true
	else
		if p then
		add(_dmpl,s)
		end
 _dmpl[#_dmpl]=_dmpl[#_dmpl]..tstr[type(str)]..(tonum(i) and '' or i)..':'..tostr(str)..' '
	p=false
	end
end
if x==0 then
foreach(_dmpl,function(v)
?v 
end)
stop()
end
end

others={}
others["nil"]=nil
others["boolean"]={true,false}
others["function"]=function()end
dmp({
[[

-----------------------------
-	dumping table values!	-
-----------------------------]]
,number={1,2,3}
,str="string"
,table={{},{'table'}}
,other=others
})

