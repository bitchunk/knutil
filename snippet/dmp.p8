--dmp [dmp() dumping table values]
--v0.4
--@shiftalow / bitchunk
function dmp(v,q,s)
	if not s then
	 q,s,_dmpx,_dmpy="\f6","\n",0,-1
	end
	local tstr,p,t={
		number="\ff#\f6:\ff"
		,string="\fc$\f6:\fc"
		,boolean="\fe%\f6:\fe"
		,["function"]="\fb*\f6:\fb"
	},s
	for i,str in pairs(type(v)=="table" and v or {v}) do
		t=type(str)
		if t=="table" then
			q,p=dmp(str,q..s..i.."{",s.." ")..s.."\f6}",s
		else
		 q..=p..i..tstr[t]..tostr(str).."\f6 "
			p=""
		end
	end
	q..=(t and "" or s.."\f2!\f6:\f2nil")
	::dmp::
	if s=="\n" and not btnp(5,flip()) then
		cls()
		?q,_dmpx*4,_dmpy*6
		_dmpx+=tonum(btn(0))-tonum(btn(1))
		_dmpy+=tonum(btn(2))-tonum(btn(3))
		goto dmp
	end
	return q
end

others={}
others["nil"]=nil --nil is 
others["boolean"]={true,false}
others["function"]=function()end
dmp({
[[

-----------------------------
  - dumping table values! -
                  ver 0.4
-----------------------------]]
,number={1,2,3}
,str="string"
,table={{},{"table"}}
,other=others
})

