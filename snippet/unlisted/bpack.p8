?'\n\n\f9★\f6 specify 32-bit values'
?'             "1" bit at a time.'
?'             \fd--val('..tostr(v,1)..')\f6'
s=''
for i,v in pairs({bunpack(v,15,unpack(split('11111111111111111111111111111111','')))}) do
?'。\+ci。\+ci。',i*3+flr((i-1)/4),106,v==1 and (i-1)%4+8 or 0
end

