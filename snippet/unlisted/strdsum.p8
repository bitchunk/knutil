function strdsum(a,b)
	local s,l,d='',max(#a,#b),''
	for i=1,l do
		local v=(a[-i] or 0)+(b[-i] or 0)+#d
		d=v>9 and '1' or '' 
		s=v%10 ..s
	end
	return d..s
end

