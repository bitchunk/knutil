--ttable [if the argument is a table, the table is returned.]
--v0.2
--@shiftalow / bitchunk

--[[
    - ttable
    - @param  any p -- check if it is a table.
    - @return any   -- if it is a table, return the argument, otherwise return false.
    - @description
    -- determines the type of a variable or argument of indeterminate type.
    -- for example, when switching the processing method between string and table.
]]--

function ttable(p)
	return count(p) and p
end

----
tbl={}
::て::
	cls()
	local r=sub(rnd(),-4)
	local ck={1,5,9}
	?'rundom value:'..r,7
	?'\n-- randomly detected chrcode --',6
	
	for i,v in pairs(ck) do
	
		-- type(tbl[i]) == 'table'
		--									and tbl[i] or {}
		tbl[i]=ttable(tbl[i]) or {}
		
		add(tbl[i],r%10==v and r or nil)
		local s=''
		foreach(tbl[i],function(a)
			s..=chr(a%240+16)
		end)
	?'\f'..ck[i]..'['..ck[i]..'] \f7'..sub(s,-14)
	end
	flip()
goto て

