foreach(h,function(v)
	?v..' ('..tostr(v,1)..')',6
end)

?'\n★1-byte delimiter(2 nibbles)',11
local h=htd(s) --args n defaults to 2
foreach(h,function(v)
	?v..' ('..tostr(v,1)..')',6
end)

?'\n★1.5-byte delimiter(3 nibbles)',11
local h=htd(s,3)
foreach(h,function(v)
	?v..' ('..tostr(v,1)..')',6
end)

