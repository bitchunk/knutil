__lua__
--comb [comb() combines two tables to create a hash table.]
--v0.1
--@shiftalow / bitchunk

function comb(k,v)
local a={}
for i=1,#k do
a[k[i]]=v[i]
end
return a
end

cls()
?"\nthe two arguments"
?" \tmust be tables of"
?" \t \t \tindex arrays.\n\n"
?"--------------------------------"
?"::combine key-table \n \t \t \tand value-table::"
?"--------------------------------"
?"key  \t:\tvalue"
?"--------------------------------"
local keys={'name','species','home','skill'}
local values={'ruth','fox','port town','kon-psi'}

local charcter=comb(keys,values)

for k,v in pairs(charcter) do
?k.." \t:\t"..v
end
