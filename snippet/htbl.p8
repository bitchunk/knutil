function htbl(ht,c)
	local t,k,rt={}
	ht,c=split(ht,'') or ht,c or 1
	while ht[c] do
		local p=ht[c]
		c+=1
		if p=='{' or p=='=' then
			rt,c=htbl(ht,c)
			if p=='=' then
				t[k],k=rt[1]
			elseif k then
				t[k],k=rt
			else
				add(t,rt)
			end
		elseif p=='}' or p==';' then
			add(t,tonorm(k))
			return t,c
		elseif p==' ' then
			add(t,tonorm(k))
			k=nil
		elseif p~="\n" then
			k=(k or '')..p
		end
	end
	add(t,tonorm(k))
	return t
end

