--tbfill [create a table by specifying initialization values and number of elements.]
--v0.3
--@shiftalow / bitchunk

--[[
	- tbfill / table fill
	- @param  number s,... starting element value of the table
	- @param  number e,... end element value of table
	- @return table / created table
	- @description / by specifying additional start and end elements for the tuple, it becomes a multidimensional table.
]]--

function tbfill(v,s,e,...)
	local t={}
	for i=s,e do
		t[i]=... and tbfill(v,...) or v
	end
	return t
end

----
cls()

t=tbfill('yes',1,3)
?t[1]..' '..t[2]..' '..t[3].."\n\n"

t=tbfill('yes',1,2,1,3)
t[2][2]='no'
?t[1][1]..' '..t[1][2]..' '..t[1][3]
?t[2][1]..' '..t[2][2]..' '..t[2][3]

colors=tbfill(14,1,3,1,3)
colors[2]=tbfill(7,1,3)
colors[3]={9,9,15}

txt='●text decoration●'
for y,cols in pairs(colors) do
	for x,c in pairs(cols) do
		?txt,24+x,58+y,c
	end
end
?txt,26,60,2
-->8
