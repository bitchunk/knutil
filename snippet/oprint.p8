pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
--oprint
--v0.3
--@shiftalow / bitchunk

--[[
	- oprint         			-- adds outlines to text and displays them.
	- @param  string s  -- text to display.
	- @param  number x  -- x coordinates.
	- @param  number y  -- y coordinates.
	- @param  number f  -- foreground color.
	- @param  number o  -- outline color.
	- @param  string p  -- p8scii code for decoration (default is outline decoration)
	- @description
		-- enclosure is not possible if tabs or newlines are included. (in pico8_v0.2.5.g)
		-- operation is not guaranteed if "param s" contains decorative p8scii.
		-- decorative parameters are "," separated, with the last character ending in a ",".
]]--

function oprint(s,x,y,f,o,p)
 for v in all(split(p or '\+ff,\+gf,\+hf,\+fg,\+hg,\+fh,\+gh,\+hh,')) do
  ?v..s,x,y,v~='' and o or f
 end
end

----

::_::
cls(0)

oprint('outline print',10,6,7,13)

oprint('shine text',16,18,(time()*8)&0xf,7)

oprint('shine outline',22,30,7,(time()*8)&0xf)

oprint('bottom shadow',28,42,7,5,'\|h,')
oprint('bottom-right shadow',34,54,12,13,'\|h,\-h,\+hh,')

r=sin(t()/10)*8
p,n='',5
for i=1,n+1 do
	s=flr(sin(t()/2+1/n*i)*r*2)+103
	c=flr(cos(t()/2+1/n*i)*r*1.5)+103
	p..='\^p\f'..select(flr(t()*24+i)%4+1,7,8,'b','c','e')..'\+'..chr(s<97 and s-39 or s)..chr(c<97 and c-39 or c)..','
end
oprint('\^w\^t    more decor',0,76,0,7,p..'\+ff,\+gf,\+hf,\+fg,\+hg,\+fh,\+gh,\+hh,')

flip()
goto _

-->8
